package com.internousdev.template.dao;

import java.sql.SQLException;

import org.apache.struts2.interceptor.SessionAware;

import com.opensymphony.xwork2.ActionSupport;

public class UserCreateCompleteDAO extends ActionSupport implements SessionAware {

	private String loginUserId;
	private String loginPasswor;
	private String UserName;
	private Map<String, Object> session;

	public String execute() throws SQLException {
		UserCreateCompleteDAO userCreateCompleteDAO = new userCreateCompleteDAO();
		userCreateCompleteDAO.createUser(session.get("loginUserId").toString(), session.get("loginPassword").toString(),
				session.get("userName").toString());
	}
}
